{
  "name": "robot-is-chill-ts",
  "version": "0.0.6",
  "description": "Robot Is Chill's TypeScript API",
  "main": "ric_api.js",
  "types": "ric_api.d.ts",
  "scripts": {
    "build": "cargo build",
    "build-ts": "npx tsc && cp src/ric_api.js src/ric_api.ts src/ric_api.d.ts .",
    "watch-try": "cargo watch -i \"**.js\" -i \"tstypes/**.d.ts\" -s \"deno run try\"",
    "try": "deno run -A npm:typescript/tsc && cargo run -- try_it_out.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ROBOT-IS-CHILL/ric-js-runtime.git"
  },
  "keywords": [
    "robot-is-chill"
  ],
  "author": "CenTdemeern1",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/ROBOT-IS-CHILL/ric-js-runtime/issues"
  },
  "homepage": "https://github.com/ROBOT-IS-CHILL/ric-js-runtime#readme",
  "devDependencies": {
    "typescript": "^5.6.3"
  },
  "files": [
    "ric_api.js",
    "ric_api.ts",
    "ric_api.d.ts"
  ]
}
